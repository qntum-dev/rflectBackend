      "checkConstraints": {
        "thumbnail_required": {
          "name": "thumbnail_required",
          "value": "\n        \"message_attachments\".\"type\" NOT IN ('image', 'video') OR \"message_attachments\".\"thumbnail_url\" IS NOT NULL\n    "
        },
        "duration_required": {
          "name": "duration_required",
          "value": "\n        \"message_attachments\".\"type\" NOT IN ('audio', 'video') OR \"message_attachments\".\"duration\" IS NOT NULL\n    "
        },
        "size_positive": {
          "name": "size_positive",
          "value": "\"message_attachments\".\"size\" > 0"
        },
        "duration_positive": {
          "name": "duration_positive",
          "value": "\n        \"message_attachments\".\"duration\" IS NULL OR \"message_attachments\".\"duration\" > 0\n    "
        },
        "size_type_limit": {
          "name": "size_type_limit",
          "value": "\n        (\"message_attachments\".\"type\" = 'image' AND \"message_attachments\".\"size\" <= 10485760) OR -- 10 MB\n        (\"message_attachments\".\"type\" = 'video' AND \"message_attachments\".\"size\" <= 104857600) OR -- 100 MB\n        (\"message_attachments\".\"type\" = 'audio' AND \"message_attachments\".\"size\" <= 52428800) OR -- 50 MB\n        (\"message_attachments\".\"type\" = 'document' AND \"message_attachments\".\"size\" <= 20971520) -- 20 MB\n    "
        }
      },
